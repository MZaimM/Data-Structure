package pendahuluanandvancesorting;

/**
 *
 * @author USER
 */
public class Quick_SortingApp {
    public static void main(String[] args) {
        int maxSize=100;
        QuickSorting arr;
        arr=new QuickSorting(maxSize);
        
        arr.insert(4);
        arr.insert(2);
        arr.insert(11);
        arr.insert(15);
        arr.insert(8);
        arr.insert(5);
        arr.insert(9);
        arr.insert(3);
        
        arr.display();
        arr.QuickSort();
        arr.display();
    }
    
    
   
    
}
